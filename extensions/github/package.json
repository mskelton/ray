{
  "name": "github",
  "description": "View GitHub repos, PRs, issues, and more.",
  "author": "mskelton",
  "license": "MIT",
  "scripts": {
    "build": "npm run generate && ray build -e dist",
    "dev": "concurrently \"ray develop\" \"npm run generate -- --watch\"",
    "lint": "ray lint",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "title": "GitHub",
  "icon": "icon.png",
  "commands": [
    {
      "name": "search-repos",
      "title": "Search Repositories",
      "subtitle": "GitHub",
      "description": "Search your repositories by name.",
      "mode": "view"
    },
    {
      "name": "my-prs",
      "title": "My Pull Requests",
      "subtitle": "GitHub",
      "description": "View pull requests you have opened.",
      "mode": "view"
    },
    {
      "name": "my-issues",
      "title": "My Issues",
      "subtitle": "GitHub",
      "description": "View issues you have created.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "title": "Personal Access Token",
      "description": "Your GitHub personal access token.",
      "placeholder": "Enter your GitHub token."
    },
    {
      "name": "query",
      "type": "textfield",
      "required": false,
      "title": "Query",
      "description": "Search query to use for all commands.",
      "placeholder": "e.g. org:Microsoft"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.1",
    "@raycast/utils": "^1.6.1",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "nearest-color": "^0.4.4",
    "octokit": "^2.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@mskelton/eslint-config": "^7.10.1",
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "^18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-sort": "^2.10.0",
    "react-devtools": "^4.27.7",
    "typescript": "^5.0.4"
  }
}
